export default class Repositorie {
    async index() {
        return ['Listando Informações'];
    }

    async create(data) {
        return { message: 'Informações criada', data };
    }

    async update(id, data) {
        return { message: `Informações ${id} atualizada`, data };
    }

    async delete(id) {
        return { message: `Informações ${id} deletada` };
    }
}
